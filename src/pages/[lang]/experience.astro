---
import { useTranslations } from '../../i18n/languagePicker'
import type { languages } from '../../i18n/languages'
import Layout from '../../layouts/Layout.astro'

export function getStaticPaths() {
	return [{ params: { lang: 'en' } }, { params: { lang: 'es' } }]
}

const { lang } = Astro.params
const t = useTranslations(lang as languages)
---

<Layout title="Ashley Lizbeth - Experience">
	<div class="flex flex-col items-center justify-center pt-28">
		<p class="text-center text-3xl text-white">{t('empty-page')}</p>
	</div>
</Layout>
